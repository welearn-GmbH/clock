{
  "name": "welearn/clock",
  "description": "Implementation of upcoming PSR-20 clock",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Welearn\\Clock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Michael Spahn",
      "email": "michael.spahn@wehorse.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "psr/clock": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml.dist"
  },
  "repositories": {
    "dev": {
      "type": "github",
      "url": "https://github.com/php-fig/clock"
    }
  },
  "provides": {
    "psr/clock-implementation": "1.0"
  }
}
